-- 코드를 입력하세요
SELECT
    HISTORY.HISTORY_ID,
    HISTORY.CAR_ID,
    TO_CHAR(HISTORY.START_DATE, 'YYYY-MM-DD') AS START_DATE,
    TO_CHAR(HISTORY.END_DATE, 'YYYY-MM-DD') AS END_DATE,
    CASE WHEN HISTORY.END_DATE - HISTORY.START_DATE +1 >= 30 THEN '장기 대여'
        ELSE '단기 대여'
    END AS RENT_TYPE
FROM CAR_RENTAL_COMPANY_RENTAL_HISTORY HISTORY
ORDER BY HISTORY_ID DESC
